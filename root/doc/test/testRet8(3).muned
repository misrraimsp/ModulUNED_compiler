(* ERROR_ret8: varias sentencias RETURN con distinto tipo de retorno en un mismo ambito *)
(* el ambito es el global *)
MODULE ret8;

VAR a:INTEGER;
	b:BOOLEAN; 

PROCEDURE fun1 (VAR a:INTEGER; VAR b:BOOLEAN):INTEGER;
	BEGIN
	
		RETURN a;
		
	END fun1;
	
PROCEDURE proc1 (VAR c:INTEGER; VAR d:BOOLEAN);
	BEGIN
	
		IF d THEN
			c:=c+1;
		ELSE
			c:=20;
		END;
		
	END proc1;

BEGIN

	RETURN a;	(*error*)
	RETURN b;	(*error*)
 	
END ret8;